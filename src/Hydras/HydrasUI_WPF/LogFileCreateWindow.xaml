<Window x:Class="HydrasUI_WPF.LogFileCreateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LogFileCreateWindow" Height="155" Width="300">
    <Grid>
        <StackPanel Margin="0,0,0,12">
            <Label Content="输入新日志文件名：" Width="212">
                
            </Label>
            <TextBox Width="209" Name="fileNameTextBox">
                
            </TextBox>
        </StackPanel>
        <WrapPanel Margin="0,54,0,40">
            <Button Content="确定" Width="90" Name="OKButton" Click="OKButton_Click"></Button>
            <Button Content="取消" Width="90" Name="CancelButton" Click="CancelButton_Click"></Button>
        </WrapPanel>
    </Grid>
</Window>
