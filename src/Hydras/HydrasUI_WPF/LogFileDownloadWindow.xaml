<Window x:Class="HydrasUI_WPF.LogFileDownloadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LogFileDownloadWindow" Height="307" Width="589">
    <StackPanel>
        <Grid>
            <Menu>
                <MenuItem Header="导出">
                    <MenuItem Header="导出文本" Click="textExport" />
                    <MenuItem Header="导出cvs" Click="excelExport" />
                </MenuItem>
            </Menu>
        </Grid>
        <Grid>
            <TextBox  Height="244" HorizontalAlignment="Left" Margin="12,12,0,0" Name="textBox" VerticalAlignment="Top" Width="543" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" />
        </Grid>
    </StackPanel>
    
</Window>
